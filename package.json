  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "codecov": "^3.8.3",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "@marzki/extensions/**/*"
  ],
  "dependencies": {
    "date-fns": "^2.27.0"
